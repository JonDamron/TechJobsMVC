<div class="container body-content">

    <h1>@ViewBag.title</h1>

    <table class="job-listing">

        @foreach (var job in ViewBag.jobs)
        {
            <tr>
                <td>
                    <b>ID: </b> @job.Id <br />
                    <b>Name: </b> @job.Name <br />
                    <b>Employer: </b> @job.Employer<br />
                    <b>Position: </b> @job.PositionType<br />
                    <b>Skill: </b> @job.CoreCompetency<br />
                </td>
            </tr>
        }


    </table>

</div>